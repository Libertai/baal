# LiberClaw production environment template
# Copy to /root/liberclaw/.env and fill in secrets

# Database
LIBERCLAW_DATABASE_URL=postgresql+asyncpg://liberclaw:CHANGE_ME@localhost/liberclaw

# JWT & encryption — generate with: python -c "import secrets; print(secrets.token_urlsafe(32))"
LIBERCLAW_JWT_SECRET=
LIBERCLAW_ENCRYPTION_KEY=

# Frontend URLs
LIBERCLAW_FRONTEND_URL=https://app.liberclaw.ai
LIBERCLAW_API_URL=https://api.liberclaw.ai
LIBERCLAW_CORS_ORIGINS=["https://app.liberclaw.ai","https://liberclaw.ai"]

# OAuth — Google
LIBERCLAW_GOOGLE_CLIENT_ID=
LIBERCLAW_GOOGLE_CLIENT_SECRET=

# OAuth — GitHub
LIBERCLAW_GITHUB_CLIENT_ID=
LIBERCLAW_GITHUB_CLIENT_SECRET=

# Magic link email
LIBERCLAW_MAGIC_LINK_SECRET=
LIBERCLAW_RESEND_API_KEY=

# Aleph Cloud deployment
ALEPH_PRIVATE_KEY=
ALEPH_SSH_PUBKEY=
ALEPH_SSH_PRIVKEY_PATH=/root/liberclaw/.ssh/id_ed25519
LIBERTAI_API_KEY=
